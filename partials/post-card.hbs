{{#match type "=" "swiper"}}
<div class="c-post-card__mobile">
  <div class="c-post-card__media">
    <a href="{{ url }}" class="c-post-card__url--cover"></a>
    {{#if feature_image}}
    <div class="c-post-card__image--wrap">
      <img class="c-post-card__image" alt="{{#if feature_image_alt}}{{ feature_image_alt }}{{ else }}{{ title }}{{/if}}"
        src="{{img_url feature_image}}" />
    </div>
    {{/if}}
  </div>
  <div class="c-post-card__mobile--main">
    <div class="c-post-card__membership">
      <div class="c-post-card__membership--label c-post-card__membership--{{visibility}}">
        {{#match visibility "paid"}}Premium{{else match visibility "members"}}Members{{else}}Free{{/match}}
      </div>
    </div>
    <div class="c-post-card__mobile--title">{{title}}</div>
    <div class="c-post-card__mobile--datas">
      <div class="c-post-card__mobile--data">
        {{#primary_tag}}
        <div>{{name}}</div>
        <div class="c-post-card__tag--name" style="margin: 0 5px;">·</div>
        {{/primary_tag}}
        <time style="font-weight: 500 !important;" datetime="{{ date format=" YYYY-MM-DD" }}" title="{{ date format="
          YYYY.MM.DD" }}">{{ date
          format="YYYY.MM.DD" }}</time>
      </div>
      <div class="c-post-card__comment--container">
        <div class="c-post-card__comment">
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" fill="#E1E1E1">
            <path
              d="M160 368c26.5 0 48 21.5 48 48v16l72.5-54.4c8.3-6.2 18.4-9.6 28.8-9.6H448c8.8 0 16-7.2 16-16V64c0-8.8-7.2-16-16-16H64c-8.8 0-16 7.2-16 16V352c0 8.8 7.2 16 16 16h96zm48 124l-.2 .2-5.1 3.8-17.1 12.8c-4.8 3.6-11.3 4.2-16.8 1.5s-8.8-8.2-8.8-14.3V474.7v-6.4V468v-4V416H112 64c-35.3 0-64-28.7-64-64V64C0 28.7 28.7 0 64 0H448c35.3 0 64 28.7 64 64V352c0 35.3-28.7 64-64 64H309.3L208 492z" />
          </svg>

        </div>
        <div class="c-post-card__comment--count">{{comment_count}}</div>
      </div>
    </div>
  </div>
</div>
{{else match type "=" "tag"}}
<a href="{{url}}">
  <div class="c-post-card__tagtype js-post-card__wrap">
    <div class="c-post-card__tagtype--main">
      <div class="c-post-card__tagtype--main--first">
        {{#primary_tag}}
        <div class="c-post-card__tagtype--tag">
          {{name}}
        </div>
        {{/primary_tag}}
        <div class="c-post-card__tagtype--title">{{title}}</div>
      </div>
      <div class="c-post-card__tagtype--main--second">
        <div class="c-post-card__tagtype--image--wrap">
          {{#if feature_image}}
          <img class="c-post-card__tagtype--image"
            alt="{{#if feature_image_alt}}{{ feature_image_alt }}{{ else }}{{ title }}{{/if}}"
            src="{{img_url feature_image}}" />
          {{/if}}
        </div>
      </div>
    </div>
    <div class="c-post-card__tagtype--data">
      <div class="c-post-card__tagtype--data--first">
        <div class="c-post-card__membership">
          <div class="c-post-card__membership--label c-post-card__membership--{{visibility}}">
            {{#match visibility "paid"}}Premium{{else match visibility "members"}}Members{{else}}Free{{/match}}
          </div>
        </div>
        <time style="font-weight: 500 !important;" datetime="{{ date format=" YYYY-MM-DD" }}" title="{{ date format="
          YYYY.MM.DD" }}">{{ date
          format="YYYY.MM.DD" }}</time>
      </div>
      <div class="c-post-card__tagtype--data--second">
        <div class="c-post-card__comment">
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" fill="#E1E1E1">
            <path
              d="M160 368c26.5 0 48 21.5 48 48v16l72.5-54.4c8.3-6.2 18.4-9.6 28.8-9.6H448c8.8 0 16-7.2 16-16V64c0-8.8-7.2-16-16-16H64c-8.8 0-16 7.2-16 16V352c0 8.8 7.2 16 16 16h96zm48 124l-.2 .2-5.1 3.8-17.1 12.8c-4.8 3.6-11.3 4.2-16.8 1.5s-8.8-8.2-8.8-14.3V474.7v-6.4V468v-4V416H112 64c-35.3 0-64-28.7-64-64V64C0 28.7 28.7 0 64 0H448c35.3 0 64 28.7 64 64V352c0 35.3-28.7 64-64 64H309.3L208 492z" />
          </svg>

        </div>
        <div class="c-post-card__comment--count">{{comment_count}}</div>
      </div>
    </div>
  </div>
</a>
{{else}}
<div class="c-post-card js-post-card__wrap">
  <div class="c-post-card__media">
    <a href="{{ url }}" class="c-post-card__url--cover"></a>
    {{#if feature_image}}
    <div class="c-post-card__image--wrap">
      <img class="c-post-card__image" alt="{{#if feature_image_alt}}{{ feature_image_alt }}{{ else }}{{ title }}{{/if}}"
        src="{{img_url feature_image}}" />
    </div>
    {{/if}}
  </div>
  <div class="c-post-card__main">
    <div class="c-post-card__membership t-v__only--desktop">
      <div class="c-post-card__membership--label c-post-card__membership--{{visibility}}">
        {{#match visibility "paid"}}Premium{{else match visibility "members"}}Members{{else}}Free{{/match}}
      </div>
    </div>
    <div class="c-post-card__content">
      <a href="{{url}}">
        <div class="c-post-card__title">{{title}}
        </div>
      </a>
    </div>

    <div class="c-post-card__data">
      <div class="c-post-card__meta">

        <div class="c-post-card__tag t-v__only--desktop">
          {{#primary_tag}}
          <div class="c-post-card__tag--name">
            <a class="c-post-card__tag--link" href="{{url}}">{{name}}</a>
          </div>
          <div class="c-post-card__tag--name" style="margin: 0 5px;">·</div>
          {{/primary_tag}}
          <div class="c-post-card__date">
            <time class="c-post-card__datetime t-v__only--desktop" datetime="{{ date format=" YYYY-MM-DD" }}"
              title="{{ date format=" YYYY년 M월 D일" }}">{{ date format="YYYY.MM.DD." }}</time>
            <time class="c-post-card__datetime t-v__only--mobile" datetime="{{ date format=" YYYY-MM-DD" }}"
              title="{{ date format=" YYYY.MM.DD" }}">{{ date format="YYYY.MM.DD" }}</time>
          </div>
        </div>
      </div>
      <div class="c-post-card__comment--container">
        <div class="c-post-card__comment">
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" fill="#E1E1E1">
            <path
              d="M160 368c26.5 0 48 21.5 48 48v16l72.5-54.4c8.3-6.2 18.4-9.6 28.8-9.6H448c8.8 0 16-7.2 16-16V64c0-8.8-7.2-16-16-16H64c-8.8 0-16 7.2-16 16V352c0 8.8 7.2 16 16 16h96zm48 124l-.2 .2-5.1 3.8-17.1 12.8c-4.8 3.6-11.3 4.2-16.8 1.5s-8.8-8.2-8.8-14.3V474.7v-6.4V468v-4V416H112 64c-35.3 0-64-28.7-64-64V64C0 28.7 28.7 0 64 0H448c35.3 0 64 28.7 64 64V352c0 35.3-28.7 64-64 64H309.3L208 492z" />
          </svg>

        </div>
        <div class="c-post-card__comment--count">{{comment_count}}</div>
      </div>
    </div>
  </div>
</div>
{{/match}}